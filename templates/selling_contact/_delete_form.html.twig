<form method="post" action="{{ path('selling_contact_delete', {'id': selling_contact.id}) }}" onsubmit="return confirm('{{ "Are you sure you want to delete this item?" | trans }}');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ selling_contact.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>

{% extends 'base.html.twig' %}

{% block title %}{{"DefaultAttribute" | trans }}{% endblock %}

{% block body %}
    <h1>{{"DefaultAttribute" | trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ default_attribute.id }}</td>
            </tr>
            <tr>
                <th>{{ "Name" | trans }}</th>
                <td>{{ default_attribute.name }}</td>
            </tr>
            <tr>
                <th>{{ "Value" | trans }}</th>
                <td>{{ default_attribute.value }}</td>
            </tr>
            <tr>
                <th>{{ "Type" | trans }}</th>
                <td>{{ default_attribute.type | trans }}</td>
            </tr>
        </tbody>
    </table>
		
		<div class="col-sm-1 col-xs-1 col-md-1 col-lg-1"></div>     
        <a href="{{ path('default_attribute_index') }}" class="btn btn-primary">{{ "back to list" | trans }}</a>
        <div class="col-sm-1 col-xs-1 col-md-1 col-lg-1"></div> 
        <a href="{{ path('default_attribute_edit', {'id': default_attribute.id}) }}" class="btn btn-primary">{{ "edit" | trans }}</a>
        <div class="col-sm-1 col-xs-1 col-md-1 col-lg-1"></div> 
        {{ include('default_attribute/_delete_form.html.twig') }}

{% endblock %}
